<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bunty.quizapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizAppnoactionbar">
        <activity android:name=".activity.activities.ProfileActivity">

        </activity>
        <activity android:name=".activity.activities.ResultActivity" />
        <activity
            android:name=".activity.activities.QuestionActivity"
            android:theme="@style/Theme.QuizApp" />
        <activity android:name=".activity.sinup_activity" />
        <activity android:name=".activity.login_activity" />
        <activity android:name=".activity.login_intro">

        </activity>
        <activity android:name=".activity.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>